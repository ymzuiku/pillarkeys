{
    "bookmarks.navigateThroughAllFiles": true,
    "bookmarks.treeview.visible": false,
    "pasteImage.path": "${projectRoot}/doc/img",
    "emmet.triggerExpansionOnTab": true,
    "omnisharp.useMono": true,
    "csharp.referencesCodeLens.enabled": false,
    "explorer.openEditors.visible": 12,
    "editor.folding": false,
    "editor.overviewRulerBorder": false,
    "editor.minimap.maxColumn": 150,
    "editor.minimap.showSlider": "always",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.wordWrap": "on",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.minimap.enabled": false,
    "editor.minimap.side": "left",
    "editor.fontSize": 14,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": false
    },
    "prettier.eslintIntegration": true,
    "window.zoomLevel": 0,
    "workbench.iconTheme": "vs-nomo-dark",
    "workbench.sideBar.location": "right",
    "workbench.editor.showTabs": true,
    "workbench.activityBar.visible": false,
    "workbench.startupEditor": "newUntitledFile",
    "terminal.external.osxExec": "/Applications/iTerm.app",
    "gitlens.advanced.blame.sizeThresholdAfterEdit": 300,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.git": false,
        "**/.svn": true,
        "**/Desktop": true,
        "**/Code": true,
        "**/Downloads": true,
        "**/Library": true,
        "**/Movies": true,
        "**/Documents": true,
        "**/Applications": true,
        "**/.Trash": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/.gitmodules": true,
        "**/*.booproj": true,
        "**/*.pidb": true,
        "**/*.suo": true,
        "**/*.user": true,
        "**/*.userprefs": true,
        "**/*.unityproj": true,
        "**/*.dll": true,
        "**/*.exe": true,
        "**/*.pdf": true,
        "**/*.mid": true,
        "**/*.midi": true,
        "**/*.wav": true,
        "**/*.3ds": true,
        "**/*.3DS": true,
        "**/*.fbx": true,
        "**/*.FBX": true,
        "**/*.lxo": true,
        "**/*.LXO": true,
        "**/*.ma": true,
        "**/*.MA": true,
        "**/*.obj": true,
        "**/*.OBJ": true,
        "**/*.asset": true,
        "**/*.cubemap": true,
        "**/*.flare": true,
        "**/*.mat": true,
        "**/*.meta": true,
        "**/*.prefab": true,
        "**/*.unity": true,
        "build/": false,
        "Build/": false,
        "Library/": true,
        "library/": true,
        "obj/": true,
        "Obj/": true,
        "ProjectSettings/": true,
        "temp/": true,
        "Temp/": true
    },
    "files.associations": {
        "*.js": "javascriptreact",
        "*.jsx": "javascriptreact"
    },
    "workbench.colorTheme": "Solarized Light"
}