{
    "pasteImage.path": "${projectRoot}/doc/img",
    "emmet.triggerExpansionOnTab": true,
    "omnisharp.useMono": true,
    "csharp.referencesCodeLens.enabled": false,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": false
      },
    "editor.acceptSuggestionOnCommitCharacter": false,
    "terminal.external.osxExec": "/Applications/iTerm.app",
    "editor.wordWrap": "on",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "prettier.eslintIntegration": true,
    "editor.fontSize": 14,
    "window.zoomLevel": 0,
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.git": false,
        "**/.svn": true,
        "**/Desktop": true,
        "**/Code": true,
        "**/Downloads": true,
        "**/Library": true,
        "**/Movies": true,
        "**/Documents": true,
        "**/Applications": true,
        "**/.Trash": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/.gitmodules":true,
        "**/*.booproj":true,
        "**/*.pidb":true,
        "**/*.suo":true,
        "**/*.user":true,
        "**/*.userprefs":true,
        "**/*.unityproj":true,
        "**/*.dll":true,
        "**/*.exe":true,
        "**/*.pdf":true,
        "**/*.mid":true,
        "**/*.midi":true,
        "**/*.wav":true,
        "**/*.3ds":true,
        "**/*.3DS":true,
        "**/*.fbx":true,
        "**/*.FBX":true,
        "**/*.lxo":true,
        "**/*.LXO":true,
        "**/*.ma":true,
        "**/*.MA":true,
        "**/*.obj":true,
        "**/*.OBJ":true,
        "**/*.asset":true,
        "**/*.cubemap":true,
        "**/*.flare":true,
        "**/*.mat":true,
        "**/*.meta":true,
        "**/*.prefab":true,
        "**/*.unity":true,
        "build/":true,
        "Build/":true,
        "Library/":true,
        "library/":true,
        "obj/":true,
        "Obj/":true,
        "ProjectSettings/":true,
        "temp/":true,
        "Temp/":true
    },
    "files.associations": {
        "*.js": "javascriptreact",
        "*.jsx": "javascriptreact"
    },
    "workbench.iconTheme": "vs-nomo-dark",
    "workbench.sideBar.location": "right",
    "editor.minimap.enabled": false,
    "workbench.editor.showTabs": false,
    "workbench.activityBar.visible": true,
    "workbench.startupEditor": "newUntitledFile",
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    }
}